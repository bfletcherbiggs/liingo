    width: 18px;
    height: 18px;
}

.utility-links div.account-icon {
    background-position: -22px -2px;
}

.utility-links div.cart-icon {
    background-position: -43px -2px;
    font-size: 12px;
    color: #f27922;
    padding-left: 7px;
    line-height: 11px;
    font-weight: 500;
}

.utility-links div.empty-cart-icon {
    background-position: -62px -2px;
}

ul.utility-links.mobile {
    margin-left: 10px;
    margin-top: 20px;
    float: right;
}

ul.utility-links.mobile li  {
