/* Nav */

.navbar {
    margin-bottom: 0px;
    border-radius: 0px;
    font-weight: medium;
}

.navbar-default {
    background-color: white;
    border: none;
    font-size: 13px;
    border-bottom: 1px solid #e0ddd7;
}

a.navbar-brand {
    height: 60px;
    padding: 0px;
    border: none;
}

.navbar-left {
    margin-left: 125px;
}

.navbar-default .navbar-left>li>a {
    color: black;
    padding: 22px 25px 10px 25px;
    letter-spacing: .09em;
    font-weight: 500;
}

.navbar-default .navbar-left>li>a:hover {
    color: #f27922;
}

.navbar-default .navbar-right>li>a  {
    color: grey;
    padding: 8px 3px 3px 0px;
    margin: 14px 5px 0 5px;
    letter-spacing: .09em;
}

.dropdown:hover .dropdown-menu {
    padding: 8px 0;
    display: block;
    border: none;
    box-shadow: none;
    font-size: 12px;
}

.dropdown-menu>li {
    display: block;
}

.dropdown-menu>li>a {
    padding: 10px 20px;
}

.dropdown-menu>li>a:hover {
    background-color: #d9d8ce;
}

.breadcrumb {
    text-align: left;
    color: #a9a5a4;
    font-size: 12px;
}

.breadcrumb a {
    color: #a9a5a4;
}

.utility-links div {
