/* browser specific styles */

    /* Large desktop */
    @media (min-width: 1200px) {

        .navbar-left {
            margin-left: 125px;
        }

    }

    /* This is that weird spot in the middle */
    @media (min-width: 1000px) and (max-width: 1200px) {


        .full.container {
            max-width: 100%;
            width: auto;
            min-width: 100%
        }


        .navbar-left {
            margin-left: 125px;
        }

    }

    /* Portrait tablet to landscape and desktop */
    @media (min-width: 768px) and (max-width: 1000px) {


        .full.container {
            max-width: 100%;
            width: auto;
            min-width: 100%
        }


        .navbar-left {
            margin-left: 20px;
        }

        .tablet-center {
            text-align: center;
        }

        .tablet-left {
            text-align: left;
        }

        .tablet-right {
            text-align: right;
        }

        div.tablet-no-pad {
            padding: 0px;
            margin: 0px;
        }

        .tablet-hidden {
            display: none;
        }

    }
    /* Landscape phone to portrait tablet */
    @media (max-width: 767px) {


        .full.container {
            max-width: 100%;
            width: auto;
            min-width: 100%
        }


        .navbar-left {
            margin-left: 0px;
        }

        .container>.navbar-header {
            margin-left: 0px;
            margin-right: 0px;
        }

        .mobile-center {
            text-align: center;
        }

        .mobile-left {
            text-align: left;
        }

        .mobile-right {
            text-align: right;
        }

        div.mobile-no-pad {
            padding: 0px;
            margin: 0px;
        }


        .tablet-hidden {
            display: none;
        }
    }

    /* Landscape phones and down */
    @media (max-width: 480px) {


        .full.container {
            max-width: 100%;
            width: auto;
            min-width: 100%
        }

        .container>.navbar-header {
            margin-left: 0px;
            margin-right: 0px;
        }

        #footer .full.container {
            padding-left: 0px;
            padding-right: 0px;
        }

        #footer .full.container .container {
            padding-left: 20px;
            padding-right: 20px;
        }

        .mobile-center {
            text-align: center;
        }

        .mobile-left {
            text-align: left;
        }

        div.mobile-no-pad {
            padding: 0px;
            margin: 0px;
        }

        .mobile-hidden {
            display: none;
        }

    }


    @media (max-width: 375px) {

        ul.utility-links.mobile li {
            margin-right: 15px;
        }

        .go-bold.header {
            font-size: 100px;
        }
    }

